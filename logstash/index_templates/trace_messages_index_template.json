{
    "index_patterns": [
        "apigw-trace-*"
    ],
    "settings": {
        "number_of_shards": 5, 
        "number_of_replicas": 2
    },
    "mappings": {
        "dynamic": false,
        "properties": {
            "@timestamp": {
                "type" : "date"
            },
            "correlationId": {
                "type": "keyword"
            },
            "level": {
                "type": "keyword"
            },
            "message": {
                "type": "text", 
                "norms": false
            }
        }
    }
}