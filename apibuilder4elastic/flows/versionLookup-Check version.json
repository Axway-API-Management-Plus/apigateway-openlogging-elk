				{
					"name": "source",
					"type": "jsonpath",
					"value": "$.params.filebeatVersion",
					"metaName": "Source",
					"metaDescription": "The input to test."
				}
			],
			"outputs": {
				"true": {
					"routes": [
						"condition.2"
					],
					"context": "$.equals",
					"metaName": "True"
				},
